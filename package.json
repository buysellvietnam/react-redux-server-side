{
  "name": "react-redux-server-side",
  "version": "0.1.0",
  "description": "An example of server-side rendered React/Redux app",
  "repository": {
    "url": "git@github.com:krasimir/react-redux-server-side.git",
    "type": "git"
  },
  "scripts": {
    "build": "browserify ./src/client/App.jsx -o ./build/client/app.js -t babelify && babel ./src/server --out-dir ./build/server",
    "watch": "concurrently \"watchify ./src/client/App.jsx -o ./build/client/app.js -t babelify -v\" \"babel ./src/server --out-dir ./build/server --watch\"",
    "start": "nodemon ./build/server/server.js"
  },
  "author": "Krasimir Tsonev <info@krasimirtsonev.com>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babelify": "8.0.0",
    "browserify": "14.5.0",
    "concurrently": "3.6.0",
    "express": "4.16.3",
    "nodemon": "1.17.5",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "watchify": "3.9.0"
  }
}
