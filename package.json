{
  "name": "react-redux-server-side",
  "version": "0.1.0",
  "description": "An example of server-side rendered React/Redux app",
  "repository": {
    "url": "git@github.com:krasimir/react-redux-server-side.git",
    "type": "git"
  },
  "scripts": {
    "build": "browserify ./src/App.jsx -o ./build/app.js -t babelify && browserify ./src/server.js -o ./build/server.js -t babelify",
    "watch": "concurrently \"watchify ./src/App.jsx -o ./build/app.js -t babelify -v\" \"watchify ./src/server.js -o ./build/server.js -t babelify -v\"",
    "start": "node ./build/server.js"
  },
  "author": "Krasimir Tsonev <info@krasimirtsonev.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babelify": "8.0.0",
    "browserify": "14.5.0",
    "concurrently": "3.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "watchify": "3.9.0"
  }
}
